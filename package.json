{
  "authors": [
    "Steve Kellock",
    "Levi Buzolic"
  ],
  "bin": {
    "osnap": "bin/osnap"
  },
  "dependencies": {
    "execa": "^7.1.0",
    "minimist": "^1.2.7",
    "tempfile": "^3.0.0",
    "which": "^3.0.0"
  },
  "description": "A CLI for grabbing ios/android screenshots and saving to your clipboard or filesystem.",
  "resolutions": {
    "got": "^11.8.5"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.14",
    "@types/which": "^3.0.0",
    "np": "^7.6.2",
    "prettier": "^2.8.1",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.3"
  },
  "files": [
    "bin/osnap",
    "macos/pbcopyimg",
    "README.md",
    "src/*.js"
  ],
  "keywords": [
    "ios",
    "android",
    "screenshot"
  ],
  "license": "MIT",
  "name": "@ferocia-oss/osnap",
  "np": {
    "yarn": true,
    "branch": "main"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ferocia/osnap.git"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc -p .",
    "build:mac": "gcc -Wall -O3 -ObjC -framework Foundation -framework AppKit -o macos/pbcopyimg macos/pbcopyimg.m",
    "clean": "rimraf {bin,src}/**/*.{d.ts,js}",
    "pretty": "prettier --no-semi --single-quote --print-width 100 --parser typescript --write src/**/*.ts",
    "watch": "tsc -p . -w",
    "test": "echo \"Error: no test specified\"",
    "prerelease": "yarn build",
    "release": "np"
  },
  "type": "commonjs",
  "version": "1.3.5"
}
